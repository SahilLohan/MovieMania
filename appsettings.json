{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionString": {
    "IMDBDatabaseConnectionString": "Data Source=YOUR_SERVER_NAME;Initial Catalog=YOUR_DATABASE_NAME;User ID=YOUR_USER_ID;Password=YOUR_PASSWORD;TrustServerCertificate=True;",
    "FirebaseConnectionString": "YOUR_FIREBASE_CONNECTION_STRING"
  },
  "JWT": {
    "ValidAudience": "User",
    "ValidIssuer": "YOUR_ISSUER_ADDRESS",
    "Secret": "YOUR_SECRET_KEY_SHOULD_BE_MORE_THAN_32_CHARACTERS"
  }

}